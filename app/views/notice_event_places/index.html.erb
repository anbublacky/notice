<h1>Listing notice_event_places</h1>

<table>
  <tr>
    <th>Event type</th>
    <th>Country</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>County</th>
    <th>Postcode</th>
    <th>Eventdate</th>
    <th>Eventtime</th>
    <th>Notice display</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @notice_event_places.each do |notice_event_place| %>
  <tr>
    <td><%= notice_event_place.event_type %></td>
    <td><%= notice_event_place.country %></td>
    <td><%= notice_event_place.address1 %></td>
    <td><%= notice_event_place.address2 %></td>
    <td><%= notice_event_place.city %></td>
    <td><%= notice_event_place.county %></td>
    <td><%= notice_event_place.postcode %></td>
    <td><%= notice_event_place.eventdate %></td>
    <td><%= notice_event_place.eventtime %></td>
    <td><%= notice_event_place.notice_display_id %></td>
    <td><%= link_to 'Show', notice_event_place %></td>
    <td><%= link_to 'Edit', edit_notice_event_place_path(notice_event_place) %></td>
    <td><%= link_to 'Destroy', notice_event_place, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

